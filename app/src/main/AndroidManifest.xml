<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.sample.snippetsproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SnippetsMenuActivity"
            android:label="Code Samples Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProgressBarActivity"
            android:label="Progress Bar Example" />
        <activity
            android:name=".RecyclerViewActivity"
            android:label="@string/title_activity_recycler_view"
            android:parentActivityName=".SnippetsMenuActivity" />
        <activity
            android:name=".FabDesignSupportLibraryActivity"
            android:label="@string/title_activity_fab_design_support_library"
            android:parentActivityName=".SnippetsMenuActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NameDialogActivity"
            android:label="Change Name Dialog"
            android:parentActivityName=".SnippetsMenuActivity" />
        <activity
            android:name=".RoomDemoActivity"
            android:label="Room Demo"
            android:parentActivityName=".SnippetsMenuActivity" />
        <activity android:name=".RecyclerviewViewTypesActivity"></activity>
    </application>

</manifest>